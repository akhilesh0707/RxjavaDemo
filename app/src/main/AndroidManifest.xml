<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aqube.rxjava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".observables.ObservablesActivity" />
        <activity android:name=".operators.OperatorsActivity" />
        <activity android:name=".observables.ObservableActivity" />
        <activity android:name=".observables.FlowableActivity" />
        <activity android:name=".observables.SingleActivity" />
        <activity android:name=".observables.MaybeActivity" />
        <activity android:name=".observables.CompletableActivity"></activity>
    </application>

</manifest>